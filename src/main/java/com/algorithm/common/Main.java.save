                if (rand.nextDouble() < 0.06) {
                    im.addEdge(i, j);
                }
            }
        }
        
        im.initializePropagationParameters();
        
        System.out.println("Network: " + im.getNumNodes() + " users, " + im.getNumEdges() + " connections");
